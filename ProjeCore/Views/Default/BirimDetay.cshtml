@using ProjeCore.Models
@model List<Personel>
@{
    ViewData["Title"] = "BirimDetay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h2>Birim Detay Sayfası</h2>
<br />
<h2>@ViewBag.brm Departmanı</h2>
<br />


<table class="table table-bordered">


    <tr>
        <th>Id</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Şehir</th>


    </tr>

    @foreach (var x in Model)
    {

        <tr>
            <td>@x.PersonelID</td>
            <td>@x.Ad</td>
            <td>@x.Soyad</td>
            <td>@x.Sehir</td>
        


        </tr>

    }


</table>

